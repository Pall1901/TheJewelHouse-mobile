    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "react": "18.3.1",
    "react-native": "0.76.4",
    "react-native-asset": "^2.1.1",
    "react-native-bootsplash": "^6.3.10",
    "react-native-device-info": "10.12.0",
